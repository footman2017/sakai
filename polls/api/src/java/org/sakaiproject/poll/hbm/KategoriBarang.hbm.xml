<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.sakaiproject.poll.model">

    <class name="org.sakaiproject.poll.model.KategoriBarang" table="uas_Jenis_Produk">

		<id name="id" type="long" column="uas_Jenis_Produk_ID">
                    <generator class="native">
                        <param name="sequence">uas_Jenis_Produk_ID_SEQ</param>
                    </generator>
		</id>
		
		<property name="Kd_Jenis" type="string" length="255" not-null="true">
                    <column name="Kd_Jenis"/>
                </property>
                
		<property name="Nama_Jenis_Produk" type="string" length="255" not-null="true">
                    <column name="Nama_Jenis_Produk"/>
                </property>	
              
	</class>

</hibernate-mapping>