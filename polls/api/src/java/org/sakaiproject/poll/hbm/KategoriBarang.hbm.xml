<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.sakaiproject.poll.model.KategoriBarang" table="uas_jenis_produk">

		<id name="id" type="long" column="KATEGORIBARANG_ID">
                    <generator class="native">
                        <param name="sequence">KATEGORI_BARANG_ID_SEQ</param>
                    </generator>
		</id>
		
		<property name="kodeJenisProduk" type="string" length="255" not-null="true">
                    <column name="KATEGORIBARANG_KODEJENIS"/>
                </property>
                
		<property name="namaJenisProduk" type="string" length="255" not-null="true">
                    <column name="KATEGORIBARANG_NAMAJENIS"/>
                </property>	
              
	</class>

</hibernate-mapping>
