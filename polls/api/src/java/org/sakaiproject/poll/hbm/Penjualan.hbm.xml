<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.sakaiproject.poll.model">

    <class name="org.sakaiproject.poll.model.Penjualan"
            table="UAS_PENJUALAN">

        <id name="Id" type="long" column="Kd_Penjualan">
            <generator class="native">
                <param name="sequence">UAS_PENJUALAN_ID_SEQ</param>
            </generator>
        </id>
        
        <property name="Kd_Customer" type="int" not-null="true">
            <column name="Kd_Customer"/>
        </property>
        
        <property name="Kd_Produk" type="int" not-null="true">
            <column name="Kd_Produk"/>
        </property>
        
        <property name="Jumlah_Barang" type="int" not-null="true">
            <column name="Jumlah_Barang"/>
        </property>
        
        <property name="Total_Biaya" type="long" not-null="true">
            <column name="Total_Biaya"/>
        </property>

    </class>
	
</hibernate-mapping>
