<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.sakaiproject.poll.model">

    <class name="org.sakaiproject.poll.model.Customer"
            table="UAS_CUSTOMER">

        <id name="Id" type="long" column="UAS_CUSTOMER_ID">
            <generator class="native">
                <param name="sequence">UAS_CUSTOMER_ID_SEQ</param>
            </generator>
        </id>
                
        <property name="Kd_Customer" type="int" not-null="true">
            <column name="Kd_Customer"/>
        </property>
        
        <property name="Nama_Customer" type="string" length="50" not-null="true">
            <column name="Nama_Customer"/>
        </property>
        
        <property name="Alamat_Customer" type="string" length="100"  not-null="true">
            <column name="Alamat_Customer"/>
        </property>
        
        <property name="No_HP_Customer" type="string" length="12"  not-null="true">
            <column name="No_HP_Customer"/>
        </property>
    </class>
	
</hibernate-mapping>
